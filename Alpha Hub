Enter.TextSize = 38.000
Enter.MouseButton1Down:Connect(function()
	if Key.Text == "TrojanHorse" then
		reminder.Text = "Right key!"
		main:Destroy()
		loadstring(game:HttpGet('https://pastebin.com/raw/bfFpEmcv'))()
	else
		local kickhanlder = game.Players.LocalPlayer
		kickhanlder:Kick('Wrong key please join https://discord.gg/82gvrCE5Ms')
		warn('Imagine not looking up the right key L')
	end
end)

